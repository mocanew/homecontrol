    "babel-preset-react": "^6.5.0",
    "colors": "^1.1.2",
    "copy": "^0.2.0",
    "eslint": "^2.10.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "node-sass": "^3.6.0",
    "nodemon": "^1.9.2",
    "npm-run-all": "^2.1.0",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react-hot-loader": "^1.3.0",
